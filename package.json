{
	"name": "@rubick/rubick-core",
	"version": "0.0.2-1",
	"description": "rubick core api",
	"author": "muwoo",
	"license": "ISC",
	"packageManager": "pnpm@6.16.0",
	"homepage": "https://github.com/rubickCenter/rubick-core#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rubickCenter/rubick-core.git"
	},
	"bugs": {
		"url": "https://github.com/rubickCenter/rubick-core/issues"
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		},
		"./*": {
			"require": "./dist/*.js",
			"import": "./dist/*.mjs"
		}
	},
	"scripts": {
		"build": "tsup-node",
		"start": "tsup-node --watch",
		"test": "jest",
		"ok": "pnpm commit && pnpm release",
		"commit": "git add . && gitmoji -c",
		"release": "release-it",
		"format": "prettier -w \"**/*.{ts,tsx,js,cjs,mjs}\" --ignore-path .gitignore"
	},
	"dependencies": {
		"cross-spawn": "^7.0.3",
		"fs-extra": "^10.0.0"
	},
	"devDependencies": {
		"@types/cross-spawn": "^6.0.2",
		"@types/fs-extra": "^9.0.13",
		"@types/jest": "^27.0.2",
		"gitmoji-changelog": "^2.2.1",
		"gitmoji-cli": "^4.6.1",
		"jest": "^27.2.4",
		"prettier": "^2.4.1",
		"release-it": "^14.11.6",
		"ts-jest": "^27.0.5",
		"tsup": "^5.2.1"
	},
	"tsup": {
		"splitting": false,
		"sourcemap": true,
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		],
		"entryPoints": [
			"src/index.ts"
		]
	},
	"release-it": {
		"npm": {
			"publish": false
		},
		"hooks": {
			"before:git:commit": "npm run format"
		}
	}
}
