    "get-mac-apps": "^1.0.2",
    "got": "^11.8.2",
    "libnpmsearch": "^3.1.2",
    "pnpm": "^6.17.2",
    "iconutil": "^1.0.2",
    "node-mac-app-icon": "^1.4.0",
    "pouchdb": "^7.2.2",
    "rubickbase": "^1.3.5",
    "yjs": "^13.5.13"
  },
  "devDependencies": {
    "@ts-type/package-dts": "^1.0.51",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/libnpmsearch": "^2.0.2",
    "@types/node": "^16.11.1",
    "@types/pouchdb": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "gitmoji-changelog": "^2.2.1",
    "gitmoji-cli": "^4.7.0",
    "jest": "^27.3.0",
    "prettier": "^2.4.1",
    "release-it": "^14.11.6",
    "ts-jest": "^27.0.7",
    "typescript": "4.5.0-dev.20211010"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "hooks": {
      "before:git:commit": "npm run lint"
    }
  }
}
