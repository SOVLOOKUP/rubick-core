{
	"name": "@rubick/rubick-core",
	"version": "0.0.2-16",
	"description": "rubick core api",
	"author": "muwoo",
	"license": "ISC",
	"homepage": "https://github.com/rubickCenter/rubick-core#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rubickCenter/rubick-core.git"
	},
	"bugs": {
		"url": "https://github.com/rubickCenter/rubick-core/issues"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"watch": "tsc --watch",
		"test": "jest",
		"ok": "npm run commit && npm run release",
		"commit": "git add . && gitmoji -c",
		"release": "release-it",
		"format": "prettier -w \"**/*.{ts,tsx,js,cjs,mjs}\" --ignore-path .gitignore"
	},
	"dependencies": {
		"pouchdb": "^7.2.2",
		"cross-spawn": "^7.0.3",
		"fs-extra": "^10.0.0",
		"got": "^11.8.2",
		"level-rocksdb": "^5.0.0",
		"pouchdb-adapter-leveldb": "^7.2.2",
		"rxdb": "^10.1.0",
		"rxjs": "^7.4.0",
		"yjs": "^13.5.13"
	},
	"devDependencies": {
		"@types/cross-spawn": "^6.0.2",
		"@types/fs-extra": "^9.0.13",
		"@types/jest": "^27.0.2",
		"@types/node": "^16.10.3",
		"@types/pouchdb": "^6.4.0",
		"@types/pouchdb-adapter-leveldb": "^6.1.3",
		"gitmoji-changelog": "^2.2.1",
		"gitmoji-cli": "^4.7.0",
		"jest": "^27.2.5",
		"prettier": "^2.4.1",
		"release-it": "^14.11.6",
		"ts-jest": "^27.0.5",
		"typescript": "4.5.0-dev.20211010"
	},
	"release-it": {
		"npm": {
			"publish": false
		},
		"hooks": {
			"before:git:commit": "npm run format"
		}
	}
}
